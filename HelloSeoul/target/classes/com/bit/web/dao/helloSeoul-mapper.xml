<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sist.spring.dao.helloSeoul">

	<select id="getDbPass" parameterType="String" resultType="String">
		select user_pw
		from userboard 
		where user_id = #{value}
	</select>
	
	<select id="getNickname" parameterType="String" resultType="String">
		select USER_NICK
		from userboard 
		where user_id = #{value}
	</select>

</mapper>


